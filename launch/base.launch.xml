<launch>
  <arg name="message_display" default="screen"/>
  <arg name="frame_id" default="imu_link"/>
  <arg name="parent_frame_id" default="base_link"/>
  
  <node pkg="mw_ahrsv1" exec="mw_ahrsv1" name="mw_ahrsv1"  namespace="mw_ahrsv1" output="screen">
  <!-- <node pkg="mw_ahrsv1" exec="mw_ahrsv1" name="mw_ahrsv1" output="$(var message_display)"> -->
    <param name="serial_port" value="/dev/recipe.driver.ahrs" />
    <param name="baud_rate" value="115200" />
    <param name="node_name" value="mw_ahrsv1" />
    <param name="frame_id" value="$(var frame_id)" />
    <param name="parent_frame_id" value="$(var parent_frame_id)" />
  </node>
</launch>
